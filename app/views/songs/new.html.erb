<div class="topic-new-wrapper" >
  <div class="container">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <h1 class="text-center">Add Picture</h1>
        <%= form_for @song do |f| %>
          <div class="form-group">
            <%= f.label :video %>
            <%= f.file_field :video%>
          </div>

          <div class="form-group">
            <%= f.label :title %>
            <%= f.text_area :title, class: 'form-control' %>
          </div>

          <%= f.submit '投稿', class: 'btn btn-black btn-block' %>

        <% end %>
      </div>
    </div>
  </div>
</div>